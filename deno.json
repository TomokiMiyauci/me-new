{
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "jsxImportSourceTypes": "@types/react",
    "types": ["vite/client", "@vitejs/plugin-rsc/types"]
  },
  "tasks": {
    "dev": "deno serve -P=dev --allow-net src/server.ts dev",
    "build": "vite build --manifest",
    "preview": "deno serve -P=preview src/server.ts"
  },
  "permissions": {
    "dev": {
      "read": true,
      "env": true,
      "sys": true,
      "ffi": true,
      "run": true,
      "write": ["."]
    },
    "preview": {
      "read": ["dist"],
      "env": true
    },
    "deploy": {
      "env": true
    }
  },
  "imports": {
    "@deno/deployctl": "jsr:@deno/deployctl@^1.13.1",
    "@miyauci/material-react": "jsr:@miyauci/material-react@^1.0.0-beta.24",
    "@std/cli": "jsr:@std/cli@^1.0.23",
    "@std/dotenv": "jsr:@std/dotenv@^0.225.5",
    "@std/http": "jsr:@std/http@^1.0.21",
    "@std/path": "jsr:@std/path@^1.1.2",
    "sanity_rsc": "./src/lib/sanity_rsc/mod.ts",
    "@/": "./src/",
    "~/": "./"
  },
  "nodeModulesDir": "auto",
  "exclude": [
    "dist"
  ],
  "deploy": {
    "exclude": [
      "**/node_modules"
    ],
    "entrypoint": "src/server.ts"
  },
  "workspace": [
    "src/lib/vite_node_builtin_scheme",
    "src/lib/vite_node_env",
    "src/lib/react_router",
    "src/lib/router",
    "src/lib/sanity_rsc",
    "src/lib/vite_rsc_assets_manifest",
    "src/lib/vite_plugin_import_meta"
  ]
}
